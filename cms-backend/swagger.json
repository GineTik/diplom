        "parameters": [],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/GetModuleDto"
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Modules"
        ]
      }
    },
    "/users/modules": {
      "get": {
        "operationId": "UsersRelatedController_getModules",
        "parameters": [],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/GetModuleDto"
                  }
                }
              }
            }
          }
        },
        "tags": [
