        ]
      }
    }
  },
  "info": {
    "title": "CMS API",
    "description": "CMS API description",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "ProfileDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "firstName": {
            "type": "string",
            "nullable": true
          },
          "lastName": {
            "type": "string",
            "nullable": true
          },
          "avatarUrl": {
            "type": "string",
            "nullable": true
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "role": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "email",
          "firstName",
          "lastName",
          "avatarUrl",
          "createdAt",
          "role"
        ]
      },
      "ErrorResponse": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "example": 401
          },
          "message": {
            "type": "string",
            "example": "Unauthorized"
          }
        },
        "required": [
          "statusCode",
          "message"
        ]
      },
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "roleSlug": {
            "type": "string"
          },
          "instanceId": {
            "type": "string"
          }
        },
        "required": [
          "firstName",
          "lastName",
          "email",
          "roleSlug",
          "instanceId"
        ]
      },
      "CreateInstanceDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "validMailDomains": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "name",
          "validMailDomains"
        ]
      },
      "ForbiddenResponse": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "example": 401
          },
          "message": {
            "type": "string",
            "example": "Unauthorized"
          },
          "requiredPermissions": {
            "example": [
              "institution:read"
            ],
            "nullable": true,
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "statusCode",
          "message",
          "requiredPermissions"
        ]
      },
      "GetInstanceDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "validMailDomains": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          }
        },
        "required": [
          "id",
          "name",
          "validMailDomains",
          "createdAt",
          "updatedAt"
        ]
      },
      "UpdateModuleDto": {
        "type": "object",
        "properties": {
          "isEnabled": {
            "type": "boolean"
          },
          "apiKey": {
            "type": "string",
            "nullable": true
          },
          "baseUrl": {
            "type": "string"
          }
        },
        "required": [
          "isEnabled",
          "apiKey",
          "baseUrl"
        ]
      },
      "GetModuleDto": {
        "type": "object",
        "properties": {
          "slug": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "apiKey": {
            "type": "string",
            "nullable": true
          },
          "baseUrl": {
            "type": "string"
          },
          "isEnabled": {
            "type": "boolean"
          }
        },
        "required": [
          "slug",
          "name",
          "description",
          "apiKey",
          "baseUrl",
          "isEnabled"
        ]
      },
      "Role": {
        "type": "string",
        "enum": [
          "student",
          "admin",
          "institution_admin"
        ],
        "description": "Available user roles in the system",
        "example": "student"
      },
      "Permission": {
        "type": "string",
        "enum": [
          "institution:manage",
          "institution:users",
          "institution:read",
          "users:manage",
          "users:read",
          "(instance):module:create",
          "(instance):module:read",
          "(instance):module:update",
          "(instance):module:delete"
        ],
        "description": "Available permissions in the system",
        "example": "(instance):module:read"
      }
    }
  }
}